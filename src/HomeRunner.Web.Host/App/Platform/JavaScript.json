            "scripts": [
                { "source": "~/App/_lib/Attentia/**/*.js" },
                { "source": "~/Vendor/platform/lib/DropdownOverlay.js" }
            ]
        },

        {
            "name": "SlidingApps.Collaboration.app.home",
            "runTime": {
                "targets": [ "debug", "release" ]            
                },
            "scripts": [
                { "source": "~/App/Home/app/service/ApplicationService.js" },
                { "source": "~/App/Home/app/part/**/*.js" }
            ]
        }
    ]
