<configuration>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>

  <appSettings>
    <add key="nhibernate.dialect" value="MsSql2008" />
    <add key="nhibernate.connectionstring" value="Password=sql.SA144;Persist Security Info=True;User ID=sa;Initial Catalog=AttentiaPoC;Data Source=PC-PETERVYVEY\DEV" />
    <add key="nhibernate.mappingAssembly" value="HomeRunner.Domain.ReadModel" />
  </appSettings>

  <log4net>
    <appender name="AmqpAppender" type="log4net.RabbitMQ.RabbitMQAppender, log4net.RabbitMQ">
      <hostName value="localhost" />
      <vhost value="platform" />
      <userName value="slidingapps" />
      <password value="slidingapps" />
      <exchange value="LoggingExchange"></exchange>
      <appId value="HomeRunner.Api.Host" />

      <exchangeProperties>
        <name value="LoggingExchange" />
        <exchangeType value="topic" />
        <durable value="true" />
      </exchangeProperties>

      <messageProperties>
        <appId value="TaskActivityService" />
        <topic type="log4net.Layout.PatternLayout">
          <conversionPattern value="TaskActivityService.Logging.%level" />
        </topic>
        <contentType type="log4net.Layout.PatternLayout">
          <conversionPattern value="application/json" />
        </contentType>
        <persistent value="false" />
        <priority type="log4net.Layout.PatternLayout">
          <conversionPattern value="0" />
        </priority>
      </messageProperties>

      <layout type="log4net.Layout.SerializedLayout, log4net.Ext.Json">
        <decorator type="log4net.Layout.Decorators.StandardTypesDecorator, log4net.Ext.Json" />
        <default />
        <!--explicit default members-->
        <remove value="message" />
        <!--remove the default preformatted message member-->
        <member value="message:messageobject" />
        <!--add raw message-->
      </layout>
    </appender>

    <root>
      <level value="ERROR" />
      <appender-ref ref="AmqpAppender" />
    </root>

  </log4net>
  
